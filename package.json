{
  "name": "true-wallet-sdk",
  "version": "0.0.0",
  "description": "True Wallet SDK to manage ERC-4337 wallet",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/TrueWallet/js-sdk.git"
  },
  "scripts": {
    "test": "cypress run",
    "build": "rm -rf dist && rollup -c --bundleConfigAsCjs",
    "start": "rollup -c --bundleConfigAsCjs -w",
    "lint": "eslint . --cache --ext js,ts",
    "example:browser": "npx http-server -c-1",
    "example:node": "node ./examples/node/index.js",
    "prepare": "husky install",
    "docs": "typedoc --plugin typedoc-plugin-markdown"
  },
  "author": "",
  "license": "GPL-3.0",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "cross-fetch": "^4.0.0",
    "cypress": "^13.2.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.0",
    "rollup": "^3.29.4",
    "rollup-plugin-dotenv": "^0.5.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.7",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "ethers": "^6.9.2"
  },
  "types": "./dist/browser/es/index.d.ts",
  "exports": [
    "./dist/browser/es/true-wallet-sdk.js"
  ]
}
